<table class="table">

	<thead>
		<tr>
			<th>Date</th>
			<th>Title</th>
			<th>Posted by:</th>
		</tr>
	</thead>

	<tbody>
		<% @posts.each do |post| %>
			<tr>
				<td><%= post.created_at %></td>
				<td><%= link_to post.title, faculty_post_path(post) %></td>
				<td><%= post.user %></td>
			</tr>
		<% end %>
	</tbody>

</table>